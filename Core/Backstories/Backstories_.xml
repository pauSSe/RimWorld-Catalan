  </AbandonedChild30>

  <AbandonedOrphan61>
    <!-- EN: abandoned orphan -->
    <title>orfe abandonat</title>
    <!-- EN: orphan -->
    <titleShort>orfe</titleShort>
    <titleFemale>orfena abandonada</titleFemale>
